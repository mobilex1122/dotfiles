{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "async": true,
  "palette": {
    "os": "#ffffff",
    "bg-path": "#ababff",
    "bg-git": "#57e6ff",
    "bg-host": "#7676ff",
    "wight": "#FFFFFF",
    "text": "#000000",
    "bg-root": "#cd5e42"
  },
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "p:bg-host",
          "foreground": "p:wight",
          "powerline_symbol": "\ue0bc",
          "style": "diamond",
          "template": " {{if .Env.IDEIcon}}{{.Env.IDEIcon}}{{else}}{{.Icon}}{{end}} ",
          "type": "os"
        },
        {
          "background": "p:bg-host",
          "foreground": "p:text",
          "powerline_symbol": "\ue0bc",
          "style": "diamond",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "p:bg-path",
          "foreground": "p:text",
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "powerline_symbol": "\ue0bc",
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:bg-git",
          "foreground": "p:text",
          "style": "powerline",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "powerline_symbol": "\ue0bc",
          "template": " {{ .HEAD }}",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#a1a1a1",
          "style": "plain",
          "template": "\ue0bc ",
          "type": "text"
        },
        {
          "foreground": "#cd5e42",
          "style": "plain",
          "template": "\udb84\udd3b ",
          "type": "root"
        },
        {
          "foreground": "#a1a1a1",
          "style": "plain",
          "template": "$",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{if .Root}} ROOT: {{end}}{{.Folder}}",
  "transient_prompt": {
    "foreground": "#a1a1a1",
    "template": " {{ .PWD }} \ue0b1 "
  },
  "final_space": true,
  "version": 3
}
